{
  "name": "rebased",
  "version": "0.0.35",
  "license": "MIT",
  "browser": {
    "crypto": false
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "npm run build:libs",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "upgrade:rebased": "npx npm-upgrade '*rebased*' && npm install",
    "upgrade:angular": "npx npm-upgrade '*angular*' && npm install",
    "release:major": "cd ./scripts && npm run release:major",
    "release:minor": "cd ./scripts && npm run release:minor",
    "release:patch": "cd ./scripts && npm run release:patch",
    "build:schematics": "tsc -p tools/tsconfig.tools.json && ./tools/copy.sh",
    "build:docs": "cd scripts/docs && npm run doc:gen && npm run doc:build",
    "build:site": "npm run build:docs && cd projects/site && npm run build && npm run deploy --prod",
    "build:libs": "./scripts/build-libs.sh",
    "parse:serve": "./scripts/parse-serve-dev.sh",
    "parse:dashboard": "parse-dashboard --dev --appId AppServer --masterKey AppParse --serverURL 'http://localhost:1337/api' --appName Rebased"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^11.0.3",
    "@angular/compiler": "^11.0.3",
    "@angular/core": "^11.0.3",
    "@angular/forms": "^11.0.3",
    "@angular/platform-browser": "^11.0.3",
    "@angular/platform-browser-dynamic": "^11.0.3",
    "@angular/router": "^11.0.3",
    "@ionic/storage": "^2.3.1",
    "@rebased/cache": "file:dist/libs/cache",
    "@rebased/core": "file:dist/libs/core",
    "@rebased/firebase": "file:dist/libs/firebase",
    "@rebased/parse": "file:dist/libs/parse",
    "@rebased/state": "file:dist/libs/state",
    "@types/lodash-es": "^4.17.4",
    "axios": "^0.19.2",
    "core-js": "^3.6.4",
    "deep-diff": "1.0.2",
    "firebase": "^8.1.2",
    "immer": "^6.0.8",
    "lodash-es": "^4.17.21",
    "parse": "^2.13.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "redux-watch": "^1.1.1",
    "rxjs": "^6.6.3",
    "sha256": "^0.2.0",
    "zone.js": "^0.10.3"
  },
  "devDependencies": {
    "@angular-builders/jest": "^8.2.0",
    "@angular-devkit/architect": "^0.900.1",
    "@angular-devkit/build-angular": "^0.1100.3",
    "@angular-devkit/core": "^11.0.3",
    "@angular-devkit/schematics": "^11.0.3",
    "@angular/cli": "11.0.3",
    "@angular/compiler-cli": "^11.0.3",
    "@angular/language-service": "^11.0.3",
    "@nrwl/jest": "8.12.4",
    "@nrwl/node": "^8.12.4",
    "@nrwl/workspace": "8.12.4",
    "@types/jasmine": "~3.6.0",
    "@types/jest": "25.1.2",
    "@types/node": "^13.7.0",
    "codelyzer": "^6.0.0",
    "jest": "25.1.0",
    "jest-preset-angular": "8.0.0",
    "jsdoc-babel": "^0.5.0",
    "jsdoc-to-markdown": "^5.0.3",
    "markdown-table": "^2.0.0",
    "ng-packagr": "^11.0.0",
    "nodemon": "^2.0.4",
    "parse-server": "^4.2.0",
    "prettier": "1.19.1",
    "rimraf": "^3.0.2",
    "shelljs": "^0.8.3",
    "standard-changelog": "2.0.21",
    "standard-version": "7.1.0",
    "ts-jest": "25.2.0",
    "ts-node": "^8.6.2",
    "tsify": "^4.0.1",
    "tslib": "^2.0.0",
    "tslint": "~6.1.0",
    "typescript": "4.0.5"
  }
}
