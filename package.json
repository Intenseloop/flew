    "upgrade:angular": "npx npm-upgrade '*angular*' && npm install",
    "upgrade:capacitor": "npx npm-upgrade '*capacitor*' && npm install",
    "rr:build:p": "ng build records --prod",
    "rr:publish": "npm run rr:build:p && cd dist/libs/records && npm publish && git push origin --tags",
    "rr:test": "ng test records",
    "rr:test:w": "ng test records --watch",
    "rr:patch": "./tools/bin/patch-rr.sh",
    "release:major": "cd ./scripts && npm run release:major",
    "release:minor": "cd ./scripts && npm run release:minor",
    "release:patch": "cd ./scripts && npm run release:patch",
    "app:build": "ng build app",
    "app:build:prod": "ng build firetask --prod && cp -rf ./dist/apps/firetask/index.html ./dist/apps/firetask/404.html",
    "app:serve:prod": "npm run app:build:prod && ./node_modules/.bin/http-server -p 8080 -c-1 ./dist/apps/firetask",
    "app:serve:ios": "ionic cap run ios --project firetask --livereload --port=8100 --address=0.0.0.0 && npx cap copy ios && npx cap open ios",
    "app:serve:ios:prod": "npm run app:build:prod && npx cap copy ios && npx cap open ios",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook",
    "schematics:build": "tsc -p tools/tsconfig.tools.json && ./tools/copy.sh",
    "mongo": "mongodb-runner start",
    "parse:serve": "parse-server --appId FiretaskServer --masterKey parseRulez --databaseURI mongodb://localhost/test",
    "parse:server": "cd apps/server && PARSE_MOUNT=/api npm run start",
    "parse:dashboard": "parse-dashboard --dev --appId FiretaskServer --masterKey parseRulez --serverURL 'http://localhost:1337/api' --appName Firetask"
  },
