    "workspace-schematic": "./node_modules/.bin/nx workspace-schematic",
    "dep-graph": "./node_modules/.bin/nx dep-graph",
    "help": "./node_modules/.bin/nx help",
    "rr:build:p": "ng build reactive-record --prod",
    "rr:release:custom": "standard-version release --release-as 1.0.1 && tools/bin/patch-warning.sh",
    "rr:release:patch": "standard-version release --release-as patch && tools/bin/patch-warning.sh",
    "rr:release:minor": "standard-version release --release-as minor && tools/bin/patch-warning.sh",
    "rr:release:major": "standard-version release --release-as major && tools/bin/patch-warning.sh",
    "rr:release:a": "standard-version release --release-as patch --prerelease alpha && tools/bin/patch-warning.sh",
    "rr:release:b": "standard-version release --release-as patch --prerelease beta && tools/bin/patch-warning.sh",
    "rr:publish": "npm run rr:build:p && cd dist/libs/reactive-record && npm publish && git push origin --tags",
    "rr:test:w": "ng test reactive-record --watch"
  },
  "private": true,
