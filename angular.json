        },
        "test": {
          "builder": "@nrwl/builders:jest",
          "options": {
            "jestConfig": "libs/records/jest.config.js",
            "tsConfig": "libs/records/tsconfig.spec.json",
            "setupFile": "libs/records/src/test-setup.ts"
          }
        }
      },
      "schematics": {
        "@nrwl/schematics:component": {
          "styleext": "scss"
        }
      }
    },
